{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/clients/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/homepage.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/homepage.html",
    "exclude": ["/api/*", "*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
